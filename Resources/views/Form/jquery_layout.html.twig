{% block jfdl_form_javascript %}
{% spaceless %}
    {% for child in form %}
        {{ jfdl_form_javascript(child) }}
    {% endfor %}
{% endspaceless %}
{% endblock jfdl_form_javascript %}

{% block field_javascript "" %}

{% block button_javascript "" %}

{% block jfdl_select2_javascript %}
    <script type="text/javascript">
    jQuery(document).ready(function($) {
        $field = $('#{{ id }}');

        {% block jfdl_select2_javascript_prototype %}
            $field.select2({{ configs|json_encode|raw }});
        {% endblock %}
    });
    </script>
{% endblock %}
